  "compilerOptions": {
    "outDir": "./obj/ts/",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["dom", "es5", "es2015"]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
